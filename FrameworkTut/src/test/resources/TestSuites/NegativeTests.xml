<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="NegativeLogInTestSuite" verbose="1" parallel="methods" thread-count="2">

    <test name="StartAllTestsFromPackageLoginPageTests-RemoteChrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.herokuapp.theinternet.loginPageTests.NegativeLogInTests"/>

        </classes>
    </test>

    <test name="StartAllTestsFromPackageLoginPageTests-RemoteFirefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.herokuapp.theinternet.loginPageTests.NegativeLogInTests"/>
        </classes>
    </test>
</suite>